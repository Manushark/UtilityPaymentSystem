@model UtilityPaymentSystem.Controllers.Report

<h1>Detalles del Reporte</h1>

<p><strong>ID:</strong> @Model.ReportId</p>
<p><strong>Fecha Generación:</strong> @Model.GeneratedDate.ToString("dd/MM/yyyy")</p>
<p><strong>Datos del Reporte:</strong> @Model.ReportData</p>

<a asp-action="Index" class="btn btn-secondary">Volver al listado</a>
<a asp-action="Delete" asp-route-id="@Model.ReportId" class="btn btn-danger">Eliminar</a>
